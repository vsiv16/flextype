{
  "name": "typescriptsuggestions",
  "displayName": "TypeScriptSuggestions",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:typescriptsuggestions.suggest-types"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typescriptsuggestions.suggest-types",
        "title": "TypeScript Suggestions: Suggest Types"
      },
      {
        "command": "typescriptsuggestions.insert-type",
        "title": "TypeScript Suggestions: Insert Type"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "typescriptsuggestions.insert-type",
          "when": "false"
        }
      ]
    },
    "keybindings": [
      {
        "key": "Shift+1",
        "command": "typescriptsuggestions.insert-type",
        "args": 1
      },
      {
        "key": "Shift+2",
        "command": "typescriptsuggestions.insert-type",
        "args": 2
      },
      {
        "key": "Shift+3",
        "command": "typescriptsuggestions.insert-type",
        "args": 3
      },
      {
        "key": "Shift+4",
        "command": "typescriptsuggestions.insert-type",
        "args": 4
      },
      {
        "key": "Shift+5",
        "command": "typescriptsuggestions.insert-type",
        "args": 5
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
