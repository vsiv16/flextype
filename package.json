{
  "name": "typescriptsuggestions",
  "displayName": "TypeScript Suggestions",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:typescriptsuggestions.suggest-types"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typescriptsuggestions.suggest-types",
        "title": "TypeScript Suggestions: Suggest Types"
      },
      {
        "command": "typescriptsuggestions.insert-type",
        "title": "TypeScript Suggestions: Insert Type"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "typescriptsuggestions.suggest-types",
          "when": "myExtension.hideActivationCommand == false"
        },
        {
          "command": "typescriptsuggestions.insert-type",
          "when": "false"
        }
      ]
    }, 
    "keybindings": [
      {
        "key": "Shift+1",
        "command": "typescriptsuggestions.insert-type",
        "args": 1
      },
      {
        "key": "Shift+2",
        "command": "typescriptsuggestions.insert-type",
        "args": 2
      },
      {
        "key": "Shift+3",
        "command": "typescriptsuggestions.insert-type",
        "args": 3
      },
      {
        "key": "Shift+4",
        "command": "typescriptsuggestions.insert-type",
        "args": 4
      },
      {
        "key": "Shift+5",
        "command": "typescriptsuggestions.insert-type",
        "args": 5
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "start-suggestion-server": "python3 -m flask run"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.32",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@angular/cli": "^13.3.5",
    "node-fetch": "^2.6.7"
  }
}
