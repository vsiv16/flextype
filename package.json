{
  "name": "flextype",
  "displayName": "FlexType",
  "description": "Machine learning-based type inference and automatic type annotation for TypeScript.",
  "version": "0.0.1",
  "publisher": "ucdavis-decal",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Other"
  ],
  "icon": "images/FlexTypeLogoSquare.png",
  "galleryBanner": {
    "color": "#8393ca",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:flextype.suggest-types"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flextype.suggest-types",
        "title": "FlexType: Suggest Types"
      },
      {
        "command": "flextype.insert-type",
        "title": "FlexType: Insert Type"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "flextype.suggest-types",
          "when": "myExtension.hideActivationCommand == false"
        },
        {
          "command": "flextype.insert-type",
          "when": "false"
        }
      ]
    },
    "keybindings": [
      {
        "key": "Shift+1",
        "command": "flextype.insert-type",
        "args": 1
      },
      {
        "key": "Shift+2",
        "command": "flextype.insert-type",
        "args": 2
      },
      {
        "key": "Shift+3",
        "command": "flextype.insert-type",
        "args": 3
      },
      {
        "key": "Shift+4",
        "command": "flextype.insert-type",
        "args": 4
      },
      {
        "key": "Shift+5",
        "command": "flextype.insert-type",
        "args": 5
      },
      {
        "key": "Shift+6",
        "command": "flextype.insert-type",
        "args": 6
      }
    ],
    "terminal": {
			"profiles": [
				{
					"id": "flextype.server-terminal-profile",
					"icon": "terminal",
					"title": "Server Terminal Profile"
				}
			]
		}
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "start-suggestion-server": "python3 -m flask run -h localhost -p 8585"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.32",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vscode/test-electron": "^2.1.3",
    "esbuild": "^0.14.39",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@angular/cli": "^13.3.5",
    "node-fetch": "^2.6.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vsiv16/typescriptsuggestions"
  },
  "homepage": "https://github.com/vsiv16/typescriptsuggestions/blob/master/README.md"
}
